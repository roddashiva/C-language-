#include<stdio.h>
void main()
{
    int m;
    printf("enter month:");
    scanf("%d",&m);
    if(m==1 || m==3 || m==5 || m==7 || m==8 ||m==10 || m==12)
    printf("this month contains 31 days =%d",m);
    else if (m==2)
    printf("this month contains 28 or 29 days=%d",m);
    else
    printf("this month contains 30 days");
}
