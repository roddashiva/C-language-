#include <stdio.h>

int main() {
    int i,n;
 int largest;
 printf("enter n:");
 scanf("%d",&n);
 int a[n];
printf("enter no in array:");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
largest=a[0];
for(i=1;i<n;i++){
    if(largest<a[i])
    largest=a[i];
    
   a[i]++;// printf("%d is largest",largest);
}

printf("%d is largest",largest);

    return 0;
}

